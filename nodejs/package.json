{
  "name": "mcp-dingtalk-doc",
  "version": "1.0.1",
  "description": "钉钉文档解析 MCP 服务器 - Node.js/TypeScript 实现，支持智能 Cookie 管理和自动登录",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "mcp-dingtalk-doc": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build",
    "prepack": "npm run build",
    "postinstall": "npx playwright install chromium --with-deps || echo 'Playwright安装可选，首次使用自动登录时会提示安装'",
    "precheck": "node scripts/pre-publish-check.js",
    "cookie:login": "tsx src/cookie-manager.ts --login",
    "cookie:check": "tsx src/cookie-manager.ts --check",
    "cookie:show": "tsx src/cookie-manager.ts --show",
    "cookie:delete": "tsx src/cookie-manager.ts --delete",
    "cookie:auto": "tsx src/cookie-manager.ts --auto",
    "playwright:install": "npx playwright install chromium"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "MCP",
    "dingtalk",
    "DingTalk",
    "钉钉",
    "钉钉文档",
    "DingTalk Docs",
    "alidocs",
    "AliDocs",
    "document-parser",
    "html-generator",
    "typescript",
    "cursor",
    "Cursor",
    "ai-tools"
  ],
  "author": "shinjiyu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hykfft/mcp-dingtalk-doc.git",
    "directory": "nodejs"
  },
  "bugs": {
    "url": "https://github.com/hykfft/mcp-dingtalk-doc/issues"
  },
  "homepage": "https://github.com/hykfft/mcp-dingtalk-doc#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "playwright": "^1.57.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
